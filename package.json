{
  "name": "devtoberfest",
  "version": "1.0.0",
  "description": "Extending your On-Premise OData Entities in CAP",
  "repository": "https://github.com/shebang-software/devtoberfest-2024",
  "license": "beerware",
  "private": true,
  "dependencies": {
    "@sap-cloud-sdk/http-client": "^3.20.0",
    "@sap-cloud-sdk/resilience": "^3.20.0",
    "@sap/cds": "^8",
    "express": "^4"
  },
  "devDependencies": {
    "@cap-js/cds-types": "^0.6",
    "@cap-js/sqlite": "^1"
  },
  "scripts": {
    "start": "cds-serve"
  },
  "cds": {
    "requires": {
      "metadata": {
        "kind": "odata",
        "model": "srv/external/metadata",
        "credentials": {
          "url": "https://services.odata.org/V4/Northwind/Northwind.svc"
        }
      }
    }
  }
}
